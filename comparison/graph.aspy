refer lib

city-map =
  [
    [ chennai -> coimbatore madurai ]
    [ coimbatore -> salem trichy ]
    [ madurai -> tanjore dindigul ]
  ]

neighbours :place :graph =
  case
    graph == []
    []
    head ( head graph ) == place
    tail ( tail ( head graph ) )
    -true-
    neighbours place ( tail graph )

neighbours coimbatore city-map
